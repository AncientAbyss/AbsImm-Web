{
  "name": "silhouette-play-react-seed",
  "version": "4.0.0",
  "description": "Silhouette Play React Seed Template",
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint --ext .js --ext .jsx bin build config server src tests",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "flow": "flow check || exit 0",
    "flow-typed:install": "flow-typed install || exit 0"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/dev-server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/dev-server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start config/karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "dependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.11.6",
    "better-npm-run": "0.0.13",
    "bootstrap-sass": "^3.3.7",
    "compression": "^1.6.2",
    "css-loader": "^0.26.0",
    "cssnano": "^3.7.4",
    "debug": "^2.2.0",
    "eslint-plugin-import": "^2.2.0",
    "extract-text-webpack-plugin": "^1.0.0",
    "fetch-mock": "^5.5.0",
    "file-loader": "^0.9.0",
    "flux-standard-action": "^1.0.0",
    "fs-extra": "^1.0.0",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.7.0",
    "ip": "^1.1.2",
    "js-cookie": "^2.1.4",
    "json-loader": "^0.5.4",
    "lodash": "^4.16.6",
    "node-sass": "^4.0.0",
    "normalize.css": "^5.0.0",
    "postcss-loader": "^1.1.0",
    "react": "^15.0.0",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.0.0",
    "react-icons": "^2.2.1",
    "react-redux": "^5.0.1",
    "react-redux-form": "^1.8.1",
    "react-router": "^3.0.0",
    "react-s-alert": "^1.2.2",
    "redux": "^3.6.0",
    "redux-actions": "^0.13.0",
    "redux-auth-wrapper": "^1.0.0",
    "redux-saga": "^0.15.1",
    "redux-thunk": "^2.0.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.6",
    "validator": "^7.0.0",
    "webpack": "^1.12.14",
    "whatwg-fetch": "^2.0.1",
    "yargs": "^6.3.0"
  },
  "devDependencies": {
